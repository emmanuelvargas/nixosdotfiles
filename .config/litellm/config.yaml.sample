# vim: ft=yaml
model_list:
  - model_name: command-r-08-2024
    litellm_params:
      model: command-r-08-2024
      api_key: COHERE_API_KEY
  - model_name: command-r-plus-08-2024
    litellm_params: &default
      model: command-r-plus-08-2024
      api_key: COHERE_API_KEY
  # ChatGPT.nvim's default model
  # https://github.com/jackMort/ChatGPT.nvim/blob/df53728e05129278d6ea26271ec086aa013bed90/lua/chatgpt/config.lua#L160
  - model_name: gpt-3.5-turbo
    litellm_params: *default
  - model_name: gpt-3.5-turbo-instruct
    litellm_params: *default
